<div class = "ver-pad">
<div class="row">
<div class="col">
<h1 class="page-title">Submissions</h1>
<div class="pad-all">
<%= form_tag("/submit", method: "get", id: "form", remote:true, multipart:true) do |f|%>
</div>
<div class="pad-all">  
<%= label_tag(:file, "File:") %>
<%= file_field_tag(:file) %>
  <br>
</div>
<div class="pad-all">
  <%= label_tag(:problem_name, "Problem:") %>
  <%= select_tag(:problem_name, options_for_select(get_shortnames)) %>
</div>
<div class="pad-all">
  <%= label_tag(:language, "Language:") %>
  <%= select_tag(:language, options_for_select([['c', 'c'], ['c++', 'cpp'], ['java', 'java']])) %>
  <br>
</div>
<div class="pad-all">
  <%= text_area_tag(:code, "Insert or upload your code") %>
  <br>
</div>
<div class="pad-all">
  <%= submit_tag("Submit", id: "test") %>
</div>
<% end %>
</div>
<div class="col">
		<div id="recently-solved">
			<h4>Your Submissions</h4>
			<%= render partial: 'shared/submissions', object: @user_attempts %>
		</div>
</div>
</div>
</div>
