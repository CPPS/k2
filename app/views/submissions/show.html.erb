<h1 class="page-title">Submissions</h1>
<div class = "ver-pad">
<%= form_tag("/search", method: "get", id: "form", remote:true, multipart:true) do |f|%>
  <%= label_tag(:file, "File:") %>
  <%= file_field_tag(:file) %>
  <%= label_tag(:problem_name, "Problem:") %>
  <%= select_tag(:problem_name, options_for_select(get_shortnames)) %>
  <%= label_tag(:language, "Language:") %>
  <%= select_tag(:language, options_for_select([['c', 'c'], ['java', 'java'], ['c++', 'c++']])) %>
  <%= submit_tag("Search", id: "test") %>
<% end %>
</div>
